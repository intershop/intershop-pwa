{
  "$schema": "http://json-schema.org/schema",
  "id": "SchematicsPWAComponent",
  "title": "PWA Lazy Components Options Schema",
  "type": "object",
  "description": "Creates Lazy Components for all decorated components.",
  "properties": {
    "project": {
      "type": "string",
      "$default": {
        "$source": "projectName"
      },
      "visible": false
    },
    "ci": {
      "type": "boolean",
      "default": false,
      "visible": false
    }
  }
}
