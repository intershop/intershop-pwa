# Editor configuration, see http://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
max_line_length = off
trim_trailing_whitespace = false

[*.less]
indent_style = tab
insert_final_newline = false
trim_trailing_whitespace = false

[*.groovy]
insert_final_newline = false
trim_trailing_whitespace = false
