{
  "name": "inspired-pwa",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:watch": "ng build --aot --watch",
    "test": "jest --ci --testResultsProcessor=jest-junit",
    "test:watch": "jest --watch",
    "lint": "ng lint",
    "format": "node node_modules/prettier/bin-prettier.js --loglevel warn --write \"**/*.*\"",
    "docs": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -d docs/compodoc -y docs/theme --hideGenerator",
    "docs:serve": "npm run docs -- -s",
    "docs:watch": "npm run docs -- -s -w",
    "e2e": "ng e2e",
    "check": "npm run lint -- --fix --format stylish && npm run format && npm run test && npm run e2e",
    "build:client-and-server-bundles": "ng build --aot --progress=false && ng run inspired-pwa:server --progress=false",
    "build:client-and-server-bundles:prod": "ng build --prod --progress=false && ng run inspired-pwa:server --progress=false",
    "build:static": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:static",
    "build:static:prod": "npm run build:client-and-server-bundles:prod && npm run webpack:server && npm run generate:static",
    "build:dynamic": "npm run build:client-and-server-bundles && npm run webpack:server",
    "build:dynamic:dev": "npm run build:dynamic",
    "build:dynamic:prod": "npm run build:client-and-server-bundles:prod && npm run webpack:server",
    "generate:static": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress=false --colors",
    "serve:static": "http-server ./dist/browser -c-1 -p 4000 --push-state",
    "serve:dynamic": "cd dist && node server",
    "start:static": "npm run build:static && npm run serve:static",
    "start:static:prod": "npm run build:static:prod && npm run serve:static",
    "start:dynamic": "npm run build:dynamic && npm run serve:dynamic",
    "start:dynamic:prod": "npm run build:dynamic:prod && npm run serve:dynamic"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.3",
    "@angular/common": "6.0.3",
    "@angular/compiler": "6.0.3",
    "@angular/core": "6.0.3",
    "@angular/forms": "6.0.3",
    "@angular/http": "6.0.3",
    "@angular/platform-browser": "6.0.3",
    "@angular/platform-browser-dynamic": "6.0.3",
    "@angular/platform-server": "6.0.3",
    "@angular/router": "6.0.3",
    "@angular/service-worker": "6.0.3",
    "@ngrx/effects": "6.0.1",
    "@ngrx/entity": "6.0.1",
    "@ngrx/store": "6.0.1",
    "@ngrx/store-devtools": "6.0.1",
    "@nguniversal/express-engine": "6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "6.0.0",
    "@ngx-translate/core": "^10.0.1",
    "@ngx-translate/http-loader": "^3.0.1",
    "angular2-uuid": "^1.1.1",
    "bootstrap": "^3.3.7",
    "core-js": "^2.5.3",
    "express": "^4.16.3",
    "ng-recaptcha": "^3.0.3",
    "ng2-validation": "^4.2.0",
    "ngrx-router": "^1.0.0",
    "ngrx-store-localstorage": "^5.0.0",
    "ngx-bootstrap": "^2.0.5",
    "rxjs": "^6.2.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "6.0.3",
    "@angular/compiler-cli": "6.0.3",
    "@angular/language-service": "6.0.3",
    "@angular-devkit/build-angular": "0.6.3",
    "@angular-devkit/build-ng-packagr": "0.6.3",
    "@angular-devkit/core": "0.6.3",
    "@angular-devkit/schematics": "0.6.3",
    "@compodoc/compodoc": "^1.1.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^6.0.0",
    "codelyzer": "4.3.0",
    "cpy-cli": "^1.0.1",
    "intershop-tslint-rules": "file:tslint-rules",
    "istanbul": "^0.4.5",
    "istanbul-api": "1.3.1",
    "istanbul-reports": "1.3.0",
    "jasmine-core": "^3.1.0",
    "jasmine-data-provider": "^2.2.0",
    "jasmine-marbles": "0.3.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jasmine-xml-reporter": "^1.1.0",
    "jest": "^23.0.0",
    "jest-junit": "^4.0.0",
    "jest-preset-angular": "^5.2.1",
    "ng-packagr": "^3.0.0-rc.2",
    "ngrx-store-freeze": "0.2.2",
    "prettier": "1.12.1",
    "protractor": "~5.3.0",
    "reflect-metadata": "^0.1.12",
    "rxjs-compat": "^6.1.0",
    "rxjs-tslint-rules": "4.1.1",
    "spa-http-server": "^0.9.0",
    "ts-loader": "4.3.0",
    "ts-mockito": "2.3.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "5.10.0",
    "tslint-clean-code": "0.2.7",
    "tslint-consistent-codestyle": "1.13.0",
    "tslint-jasmine-rules": "1.3.2",
    "typescript": "~2.7.2",
    "webpack-cli": "^2.1.3"
  }
}
