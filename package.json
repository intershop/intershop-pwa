{
  "name": "proof-of-concept",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ts-node node_modules/tslint/bin/tslint --project ./src/tsconfig.spec.json --type-check --format stylish",
    "e2e": "ng e2e",
    "build:client-and-server-bundles": "ng build --aot --progress=false && ng build --aot --app 1 --output-hashing=false --progress=false",
    "build:client-and-server-bundles:prod": "ng build --prod --progress=false && ng build --prod --app 1 --output-hashing=false --progress=false",
    "build:static": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:static",
    "build:dynamic": "npm run build:client-and-server-bundles && npm run webpack:server",
    "build:dynamic:prod": "npm run build:client-and-server-bundles:prod && npm run webpack:server:prod",
    "generate:static": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress=false --colors",
    "webpack:server:prod": "webpack --config webpack.server.prod.config.js --progress=false --colors",
    "serve:static": "cd dist/browser && http-server",
    "serve:dynamic": "cd dist && node server",
    "start:static": "npm run build:static && npm run serve:static -- -p 4000",
    "start:dynamic": "npm run build:dynamic && npm run serve:dynamic",
    "start:dynamic:prod": "npm run build:dynamic:prod && npm run serve:dynamic"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/platform-server": "^4.2.4",
    "@angular/router": "^4.2.4",
    "@nguniversal/express-engine": "^1.0.0-beta.3",
    "@nguniversal/module-map-ngfactory-loader": "^1.0.0-beta.3",
    "@ngx-translate/core": "^8.0.0",
    "@ngx-translate/http-loader": "^2.0.0",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "crosstab": "^0.2.12",
    "crypto-js": "^3.1.9-1",
    "intl": "^1.2.5",
    "lzutf8": "^0.4.4",
    "ng-recaptcha": "^2.1.1",
    "ng2-completer": "^1.6.0",
    "ng2-validation": "^4.2.0",
    "ngx-bootstrap": "^1.8.1",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.3.2",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@types/crypto-js": "^3.1.33",
    "@types/jasmine": "2.5.45",
    "@types/lodash": "^4.14.73",
    "@types/node": "^8.0.30",
    "codelyzer": "~3.1.1",
    "cpy-cli": "^1.0.1",
    "dependency-cruiser": "^2.3.2",
    "http-server": "^0.10.0",
    "jasmine-core": "~2.6.2",
    "jasmine-data-provider": "^2.2.0",
    "jasmine-spec-reporter": "~4.1.0",
    "jasmine-xml-reporter": "^1.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "0.0.31",
    "protractor": "~5.1.2",
    "reflect-metadata": "^0.1.10",
    "ts-loader": "^2.3.7",
    "ts-mockito": "^2.1.0",
    "ts-node": "~3.2.0",
    "tslint": "^5.7.0",
    "typescript": "~2.3.3"
  }
}
