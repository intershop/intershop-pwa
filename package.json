{
  "name": "proof-of-concept",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ts-node node_modules/tslint/bin/tslint --project ./tsconfig.tslint.json --format stylish",
    "e2e": "ng e2e",
    "build:client-and-server-bundles": "ng build --aot --progress=false && ng build --aot --app 1 --output-hashing=false --progress=false",
    "build:client-and-server-bundles:prod": "ng build --prod --progress=false && ng build --prod --app 1 --output-hashing=false --progress=false",
    "build:static": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:static",
    "build:dynamic": "npm run build:client-and-server-bundles && npm run webpack:server",
    "build:dynamic:dev": "npm run build:dynamic",
    "build:dynamic:prod": "npm run build:client-and-server-bundles:prod && npm run webpack:server",
    "generate:static": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress=false --colors",
    "serve:static": "cd dist/browser && http-server . -c-1 -p 4000 --push-state",
    "serve:dynamic": "cd dist && node server",
    "start:static": "npm run build:static && npm run serve:static",
    "start:dynamic": "npm run build:dynamic && npm run serve:dynamic",
    "start:dynamic:prod": "npm run build:dynamic:prod && npm run serve:dynamic"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.2",
    "@angular/common": "^5.2.2",
    "@angular/compiler": "^5.2.2",
    "@angular/core": "^5.2.2",
    "@angular/forms": "^5.2.2",
    "@angular/http": "^5.2.2",
    "@angular/platform-browser": "^5.2.2",
    "@angular/platform-browser-dynamic": "^5.2.2",
    "@angular/platform-server": "^5.2.2",
    "@angular/router": "^5.2.2",
    "@ngrx/effects": "^5.0.1",
    "@ngrx/entity": "^5.1.0",
    "@ngrx/router-store": "^5.0.1",
    "@ngrx/store": "^5.0.0",
    "@ngrx/store-devtools": "^5.0.1",
    "@nguniversal/express-engine": "^5.0.0-beta.5",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
    "@ngx-translate/core": "^9.1.1",
    "@ngx-translate/http-loader": "^2.0.1",
    "angular2-uuid": "^1.1.1",
    "bootstrap": "^3.3.7",
    "core-js": "^2.5.3",
    "crosstab": "^0.2.12",
    "crypto-js": "^3.1.9-1",
    "intl": "^1.2.5",
    "lzutf8": "^0.4.7",
    "ng-recaptcha": "^3.0.3",
    "ng2-completer": "^1.6.3",
    "ng2-validation": "^4.2.0",
    "ngx-bootstrap": "^2.0.2",
    "ngx-cookie": "^2.0.1",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.6",
    "@angular/compiler-cli": "^5.2.2",
    "@angular/language-service": "^5.2.2",
    "@types/crypto-js": "^3.1.38",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.1.0",
    "cpy-cli": "^1.0.1",
    "dependency-cruiser": "^2.12.1",
    "jasmine-core": "~2.9.1",
    "jasmine-data-provider": "^2.2.0",
    "jasmine-marbles": "^0.2.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jasmine-xml-reporter": "^1.1.0",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "karma-spec-reporter": "0.0.32",
    "ngrx-store-freeze": "^0.2.1",
    "protractor": "~5.3.0",
    "reflect-metadata": "^0.1.12",
    "rxjs-tslint-rules": "^3.7.2",
    "spa-http-server": "^0.9.0",
    "ts-loader": "^3.3.1",
    "ts-mockito": "2.2.7",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.6.2",
    "typescript-formatter": "^7.0.1"
  }
}
