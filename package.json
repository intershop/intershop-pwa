{
  "name": "intershop-pwa",
  "description": "Intershop Progressive Web App",
  "version": "0.7.10",
  "license": "Intershop Standard Software End User License Agreement Intershop 7",
  "keywords": [
    "intershop"
  ],
  "engines": {
    "node": ">=10.13.0",
    "npm": "~6.4.1"
  },
  "private": true,
  "scripts": {
    "ng": "npx ng",
    "start": "npx ng serve",
    "build": "npx ng build",
    "build:watch": "npx ng build --aot --watch",
    "test": "jest --ci",
    "test:watch": "jest --watch",
    "e2e": "npx cypress open",
    "lint": "npx ng lint",
    "stylelint": "stylelint \"src/theme/**/*.scss\" --fix",
    "format": "node node_modules/prettier/bin-prettier.js --loglevel warn --write \"**/*.*\"",
    "check": "npm run lint -- --fix --format stylish && npm run format && npm run stylelint && npx tsc --project src/tsconfig.spec.json && npm run build:dynamic:prod && npm run test",
    "changelog": "npx conventional-changelog -t RELEASE_ -n intershop-changelog.js -i CHANGELOG.md -s",
    "3rd-party-licenses": "license-checker --csv --out 3rd-party-licenses.txt --customPath templates/3rd-party-licenses_format.json",
    "3rd-party-licenses:summary": "license-checker --summary",
    "docs": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -d docs/compodoc -y docs/theme --hideGenerator",
    "docs:serve": "npm run docs -- -s",
    "docs:watch": "npm run docs -- -s -w",
    "build:client-and-server-bundles": "npx ng build --aot --progress=false && npx ng run intershop-pwa:server --progress=false",
    "build:client-and-server-bundles:prod": "npx ng build --prod --progress=false && npx ng run intershop-pwa:server:production --progress=false",
    "build:static": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:static",
    "build:static:prod": "npm run build:client-and-server-bundles:prod && npm run webpack:server && npm run generate:static",
    "build:dynamic": "npm run build:client-and-server-bundles && npm run webpack:server",
    "build:dynamic:dev": "npm run build:dynamic",
    "build:dynamic:prod": "npm run build:client-and-server-bundles:prod && npm run webpack:server",
    "generate:static": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress=false --colors",
    "serve:static": "http-server ./dist/browser -c-1 -p 4200 --push-state",
    "serve:dynamic": "cd dist && node server",
    "start:static": "npm run build:static && npm run serve:static",
    "start:static:prod": "npm run build:static:prod && npm run serve:static",
    "start:dynamic": "npm run build:dynamic && npm run serve:dynamic",
    "start:dynamic:prod": "npm run build:dynamic:prod && npm run serve:dynamic"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^0.11.2",
    "@angular-devkit/core": "^7.1.2",
    "@angular-devkit/schematics": "^7.1.2",
    "@angular/animations": "^7.1.2",
    "@angular/cli": "~7.1.2",
    "@angular/common": "^7.1.2",
    "@angular/compiler": "^7.1.2",
    "@angular/compiler-cli": "^7.1.2",
    "@angular/core": "^7.1.2",
    "@angular/forms": "^7.1.2",
    "@angular/http": "^7.1.2",
    "@angular/language-service": "^7.1.2",
    "@angular/platform-browser": "^7.1.2",
    "@angular/platform-browser-dynamic": "^7.1.2",
    "@angular/platform-server": "^7.1.2",
    "@angular/router": "^7.1.2",
    "@angular/service-worker": "^7.1.2",
    "@fortawesome/angular-fontawesome": "0.3.0",
    "@fortawesome/fontawesome-svg-core": "1.2.9",
    "@fortawesome/free-solid-svg-icons": "5.6.0",
    "@ng-bootstrap/ng-bootstrap": "^4.0.0",
    "@ngrx/effects": "6.1.2",
    "@ngrx/entity": "6.1.2",
    "@ngrx/store": "6.1.2",
    "@ngrx/store-devtools": "6.1.2",
    "@nguniversal/express-engine": "7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "7.0.2",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@schematics/angular": "^7.1.2",
    "@types/lodash-es": "^4.17.1",
    "@types/node": "^10.12.12",
    "@wishtack/reactive-component-loader": "^0.2.0",
    "@wishtack/rx-scavenger": "^1.0.2",
    "angular2-uuid": "^1.1.1",
    "b64u": "^2.0.0",
    "bootstrap": "^4.1.3",
    "core-js": "^2.6.0",
    "express": "^4.16.4",
    "lodash-es": "^4.17.11",
    "ng-dynamic-component": "^4.0.0",
    "ng-recaptcha": "^4.2.1",
    "ng2-validation": "^4.2.0",
    "ngrx-router": "^2.0.1",
    "ngrx-store-localstorage": "^5.1.0",
    "ngx-infinite-scroll": "^6.0.1",
    "rxjs": "^6.3.3",
    "ts-loader": "5.3.1",
    "typescript": "~3.1.6",
    "webpack-cli": "^3.1.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.7",
    "@cypress/webpack-preprocessor": "^4.0.2",
    "codelyzer": "^4.5.0",
    "conventional-changelog-cli": "^2.0.11",
    "cpy-cli": "^2.0.0",
    "cypress": "^3.1.3",
    "htmlify-csv": "^1.0.1",
    "husky": "^1.2.0",
    "intershop-tslint-rules": "file:tslint-rules",
    "istanbul-api": "2.0.6",
    "istanbul-reports": "2.0.1",
    "jasmine-data-provider": "^2.2.0",
    "jest": "^23.6.0",
    "jest-extended": "^0.11.0",
    "jest-html-reporter": "^2.4.2",
    "jest-junit": "^5.2.0",
    "jest-marbles": "^2.1.0",
    "jest-preset-angular": "^6.0.2",
    "jscpd-html-reporter": "^2.0.1",
    "license-checker": "^24.0.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "reflect-metadata": "^0.1.12",
    "rxjs-compat": "^6.3.3",
    "rxjs-tslint-rules": "^4.13.1",
    "spa-http-server": "^0.9.0",
    "stylelint": "^9.9.0",
    "stylelint-config-recess-order": "^2.0.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.4.1",
    "ts-mockito": "2.3.1",
    "tsickle": "^0.34.0",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "tslint-clean-code": "^0.2.7",
    "tslint-consistent-codestyle": "^1.13.3",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-etc": "^1.2.4",
    "tslint-html-report": "^2.0.2",
    "tslint-jasmine-rules": "1.4.0",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
